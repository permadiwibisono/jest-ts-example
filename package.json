{
  "name": "jest-ts-example",
  "version": "1.0.0",
  "description": "jest typescript example",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/permadiwibisono/jest-ts-example.git"
  },
  "keywords": [],
  "author": "Permadi Wibisono",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/permadiwibisono/jest-ts-example/issues"
  },
  "homepage": "https://github.com/permadiwibisono/jest-ts-example#readme",
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf dist",
    "start": "webpack serve",
    "watch": "webpack --watch",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack",
    "build:stats": "npm run build -- --json stats.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore -w \"**/*.+(js|jsx|ts|tsx|json|scss|css|html|md)\"",
    "format:check": "prettier --ignore-path .gitignore -l \"**/*.+(js|jsx|ts|tsx|json|scss|css|html|md)\"",
    "test": "jest"
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx|json|scss|css|html|md)": [
      "prettier -w"
    ],
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/node": "^20.1.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.0",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.8",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "typescript": "^5.0.4",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "husky": "^7.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
